@{
    ViewBag.Title = "infomhs";
}


@model WebApps.Models.mahasiswa
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width">
    <title>@ViewBag.Title - Lepkom App</title>
    <link rel="stylesheet" href="../../Content/StyleSheet1.css" type="text/css" />

</head>
<body>
   <div class="form1">
       <h3>Info Detail Untuk NPM
           <u>@ViewBag.display_npm</u>
       </h3>
       <table border="1">
           <tbody>
                <tr>
                    <td rowspan="5"><img src="../../Content/image.jpg" /></td>
                    <td>Nama : @Model.nama</td>
                </tr>
                <tr><td>NPM  :@Model.npm</td></tr>
                <tr><td>Kelas  :@Model.kelas</td></tr>
                <tr><td>Jurusan  :@Model.jurusan</td></tr>
                <tr><td>Semester  :@Model.semester</td></tr>
           </tbody>
       </table>
       <br />
       <button><a href="/">Back</a></button>
   </div>
    
</body>
</html>
